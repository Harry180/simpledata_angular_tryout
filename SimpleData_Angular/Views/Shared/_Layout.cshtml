<!DOCTYPE html>
<html ng-app="karolSeals">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<title>@ViewBag.Title</title>
	@Styles.Render("~/Content/css")
	<link type="text/css" rel="stylesheet" href="@Url.Content("~/Content/bootstrap.css")" />
	<link type="text/css" rel="stylesheet" href="@Url.Content("~/Content/main.css")" />
	@Scripts.Render("~/bundles/modernizr")
</head>
<body>
	<div class="wrapper">
		<div class="container">
			<div class="row">
			</div>
		</div>
		<div class="container">
			@RenderBody()
		</div>
	</div>
	@Scripts.Render("~/bundles/jquery")
	<script type="text/javascript" src="@Url.Content("~/Scripts/angular.js")"></script>
	<script type="text/javascript" src="@Url.Content("~/Scripts/app.js")"></script>
	@RenderSection("scripts", required: false)
</body>
</html>
